    "instruction": "cat /etc/oracle-release",
    "output": " &> /dev/null)\n\tOracleHome=\"/u01/app/oracle\"\n\tOracleVersion=\"19.3.0.0\"\n\tWebLogicServer=\"wlserver_10.3.6.0\"\nfi\n\n# Check if Oracle database is running\nfunction check_oracle_db_running() {\n    local ORACLE_STATUS=$(su - oracle -c \"dbcsa - Version\")\n    if [[ $ORACLE_STATUS == *\"database is up\"* ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if WebLogic server is running\nfunction check_weblogic_server_running() {\n    local WLS_STATUS=$(su - oracle -c \"/usr/local/bin/wls -version\")\n    if [[ $WLS_STATUS == *\"WebLogic Server\"* ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if Node Manager is running\nfunction check_node_manager_running() {\n    local NM_STATUS=$(su - oracle -c \"/usr/local/bin/nodemanager -version\")\n    if [[ $NM_STATUS == *\"Node Manager\"* ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if Oracle database is affected by ORA-600 [Ueditor1 Insufficient System Resources]\nfunction check_ora_600() {\n    local ORACLE_STATUS=$(su - oracle -c \"dbcsa - Version\")\n    if [[ $ORACLE_STATUS == *\"database is up\"* ]] && [[ $ORACLE_STATUS =~ ORA-600 ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the Node Manager process is running\nfunction check_node_manager_process_running() {\n    local NM_PROCESS=$(su - oracle -c \"/usr/local/bin/ps -ef | grep nodemanager\")\n    if [[ -n $NM_PROCESS ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the WebLogic server is running with the desired version\nfunction check_weblogic_server_version() {\n    local WLS_VERSION=$(su - oracle -c \"/usr/local/bin/wls -version\")\n    if [[ $WLS_VERSION == *\"$WebLogicServer\"* ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the Oracle database is running with the desired version\nfunction check_oracle_database_version() {\n    local ORACLE_VERSION=$(su - oracle -c \"dbcsa - Version\")\n    if [[ $ORACLE_VERSION == *\"$OracleVersion\"* ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the WebLogic server has the desired number of CPU threads\nfunction check_weblogic_server_cpu_threads() {\n    local WLS_CPU_THREADS=$(su - oracle -c \"/usr/local/bin/wls -largethreads\")\n    if [[ $WLS_CPU_THREADS -ge $WebLogicCpuThreads ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the WebLogic server has the desired number of CPU threads\nfunction check_weblogic_server_large_threads() {\n    local WLS_LARGE_THREADS=$(su - oracle -c \"/usr/local/bin/wls -largethreads\")\n    if [[ $WLS_LARGE_THREADS -ge $WebLogicLargeThreads ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the Node Manager process has the desired memory usage\nfunction check_node_manager_memory_usage() {\n    local NM_MEMORY_USAGE=$(su - oracle -c \"/usr/local/bin/ps -ef | grep nodemanager | awk '{print $4}'\")\n    if [[ $NM_MEMORY_USAGE -le $NodeManagerMemoryUsage ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Create a hash for theWebLogic server and Node Manager process\nfunction create_hash() {\n    local HASH=$(echo -n \"${WebLogicServer}${NodeManagerProcess}\" | sha256sum)\n    echo ${HASH}\n}\n\n# Validate the configuration of the WebLogic server and Node Manager process\nfunction validate_config() {\n    local WLS_CONFIG_VALID=$(su - oracle -c \"/usr/local/bin/wls -configValidation\")\n    if [[ $WLS_CONFIG_VALID == *\"Parameter\"* ]]; then\n        echo \"1\"\n    else\n        echo \"0\"\n    fi\n}\n\n# Check if the Oracle database is running with the desired version and the WebLogic server is running with the desired version and CPU threads\nfunction check_database_and_weblogic_server() {\n    local ORACLE_DB_RUNNING=$(check_oracle_db_running)\n    local WLS_RUNNING=$(check_weblogic_server_running)\n    