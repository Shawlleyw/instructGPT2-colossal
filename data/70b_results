  {
    "instruction": "Regex matching for letters, numbers and whitespace",
    "output": "\n        Description: Use regular expression to match letters, numbers and whitespace\n        :param line: input line to be matched\n        :return: True if line contains only letters, numbers and whitespace, False otherwise\n        \"\"\"\n        return re.match(r'[\\w\\s]+', line)\n\n    @staticmethod\n    def is_ comment(line):\n        \"\"\"\n        Description: Check if a line is a comment\n        :param line: input line to be checked\n        :return: True if line is a comment, False otherwise\n        \"\"\"\n        return line.startswith('#') or line.startswith(';')\n\n    def __get_tokens(self, line):\n        \"\"\"\n        Description: Tokenize a line\n        :param line: input line to be tokenized\n        :return: list of tokens\n        \"\"\"\n        return re.split(r'\\W+', line)\n\n    def __is_ ws(self, token):\n        \"\"\"\n        Description: Check if a token is whitespace\n        :param token: input token to be checked\n        :return: True if token is whitespace, False otherwise\n        \"\"\"\n        return token.isspace()\n\n    def __is_ identifier(self, token):\n        \"\"\"\n        Description: Check if a token is an identifier\n        :param token: input token to be checked\n        :return: True if token is an identifier, False otherwise\n        \"\"\"\n        return token.isalpha() or token == '_'\n\n    def __is_ symbol(self, token):\n        \"\"\"\n        Description: Check if a token is a symbol\n        :param token: input token to be checked\n        :return: True if token is a symbol, False otherwise\n        \"\"\"\n        return token.isalpha() or token.isdigit() or token == '_' or token == '$'\n\n    def __is_ number(self, token):\n        \"\"\"\n        Description: Check if a token is a number\n        :param token: input token to be checked\n        :return: True if token is a number, False otherwise\n        \"\"\"\n        try:\n            float(token)\n            return True\n        except ValueError:\n            return False\n\n    def __is_ string(self, token):\n        \"\"\"\n        Description: Check if a token is a string\n        :param token: input token to be checked\n        :return: True if token is a string, False otherwise\n        \"\"\"\n        return token[0] == '\"' or token[0] == \"'\"\n\n    def __parse_expression(self, tokens):\n        \"\"\"\n        Description: Parse an expression\n        :param tokens: list of tokens to be parsed\n        :return: parsed expression\n        \"\"\"\n        expression = []\n        for token in tokens:\n            if self.__is_ symbol(token):\n                expression.append(token)\n            elif self.__is_ number(token):\n                expression.append(token)\n            elif self.__is_ string(token):\n                expression.append(token)\n            elif self.__is_ identifier(token):\n                expression.append(token)\n            else:\n                raise SyntaxError(\"Invalid token: {}\".format(token))\n        return expression\n\n    def __parse_statements(self, tokens):\n        \"\"\"\n        Description: Parse statements\n        :param tokens: list of tokens to be parsed\n        :return: list of statements\n        \"\"\"\n        statements = []\n        for token in tokens:\n            if self.__is_ symbol(token):\n                statements.append(self.__parse_expression(token))\n            elif self.__is_ identifier(token):\n                statements.append(token)\n            else:\n                raise SyntaxError(\"Invalid token: {}\".format(token))\n        return statements\n\n    def parse(self, code):\n        \"\"\"\n        Description: Parse code\n        :param code: input code to be parsed\n        :return: list of statements\n        \"\"\"\n        tokens = self.__get_tokens(code)\n        statements = self.__parse_statements(tokens)\n        return statements\n\n    def __str__(self):\n        return \"Lexer\"\n\n\nclass Parser:\n    \"\"\"\n    Description: Parse code and generate abstract syntax tree\n    \"\"\"\n\n    def __init__(self, lexer):\n        \"\"\"\n        Description: Initialize parser\n        :param lexer: lexer object\n        \"\"\"\n        self.lexer = lexer\n\n    def parse(self, code):\n        \"\"\"\n        Description: Parse code and generate abstract syntax tree\n        :param code: input code to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        statements = self.lexer.parse(code)\n        ast = self.__parse_statements(statements)\n        return ast\n\n    def __parse_statements(self, statements):\n        \"\"\"\n        Description: Parse statements\n        :param statements: list of statements to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        ast = []\n        for statement in statements:\n            if statement[0] == 'expression':\n                ast.append(self.__parse_expression(statement))\n            elif statement[0] == 'identifier':\n                ast.append(self.__parse_identifier(statement))\n            else:\n                raise SyntaxError(\"Invalid statement: {}\".format(statement))\n        return ast\n\n    def __parse_expression(self, expression):\n        \"\"\"\n        Description: Parse expression\n        :param expression: expression to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        ast = []\n        for token in expression:\n            if token[0] == 'symbol':\n                ast.append(self.__parse_symbol(token))\n            elif token[0] == 'number':\n                ast.append(self.__parse_number(token))\n            elif token[0] == 'string':\n                ast.append(self.__parse_string(token))\n            elif token[0] == 'identifier':\n                ast.append(self.__parse_identifier(token))\n            else:\n                raise SyntaxError(\"Invalid token: {}\".format(token))\n        return ast\n\n    def __parse_symbol(self, symbol):\n        \"\"\"\n        Description: Parse symbol\n        :param symbol: symbol to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        return Symbol(symbol[1])\n\n    def __parse_number(self, number):\n        \"\"\"\n        Description: Parse number\n        :param number: number to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        return Number(number[1])\n\n    def __parse_string(self, string):\n        \"\"\"\n        Description: Parse string\n        :param string: string to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        return String(string[1])\n\n    def __parse_identifier(self, identifier):\n        \"\"\"\n        Description: Parse identifier\n        :param identifier: identifier to be parsed\n        :return: abstract syntax tree\n        \"\"\"\n        return Identifier(identifier[1])\n\n    def __str__(self):\n        return \"Parser\"\n\n\nclass Symbol:\n    \"\"\"\n    Description: Represent a symbol\n    \"\"\"\n\n    def __init__(self, name):\n        \"\"\"\n        Description: Initialize symbol\n        :param name: name of the symbol\n        \"\"\"\n        self.name = name\n\n    def __str__(self):\n        return self.name\n\n\nclass Number:\n    \"\"\"\n    Description: Represent a number\n    \"\"\"\n\n    def __init__(self, value):\n        \"\"\"\n        Description: Initialize number\n        :param value: value of the number\n        \"\"\"\n        self.value = value\n\n    def __str__(self):\n        return str(self.value)\n\n\nclass String:\n    \"\"\"\n    Description: Represent a string\n    \"\"\"\n\n    def __init__(self, value):\n        \"\"\"\n        Description: Initialize string\n        :param value: value of the string\n        \"\"\"\n        self.value = value\n\n    def __str__(self):\n        return self.value\n\n\nclass Identifier:\n    \"\"\"\n    Description: Represent an identifier\n    \"\"\"\n\n    def __init__(self, name):\n        \"\"\"\n        Description: Initialize identifier\n        :param name: name of the identifier\n        \"\"\"\n        self.name = name\n\n    def __str__(self):\n        return self.name\n\n\nclass AST:\n    \"\"\"\n    Description: Represent an abstract syntax tree\n    \"\"\"\n\n    def __init__(self, type, value):\n        \"\"\"\n        Description: Initialize abstract syntax tree\n        :param type: type of the ast node\n        :param value: value of the ast node\n        \"\"\"\n        self.type = type\n        self.value = value\n\n    def __str__(self):\n        return \"Type: {} Value: {}\".format(self.type, self.value)\n\n\nclass Interpreter:\n    \"\"\"\n    Description: Interpret abstract syntax tree\n    \"\"\"\n\n    def __init__(self, ast):\n        \"\"\"\n        Description: Initialize interpreter\n        :param ast: abstract syntax tree to be interpreted\n        \"\"\"\n        self.ast = ast\n\n    def interpret(self):\n        \"\"\"\n        Description: Interpret abstract syntax tree\n        :return: result of interpretation\n        \"\"\"\n        return self.interpret_ast(self.ast)\n\n    def interpret_ast(self, ast):\n        \"\"\"\n        Description: Interpret abstract syntax tree\n        :param ast: abstract syntax tree to be interpreted\n        :return: result of interpretation\n        \"\"\"\n        if ast.type == 'expression':\n            return self.interpret_expression(ast.value)\n        elif ast.type == 'identifier':\n            return self.interpret_identifier(ast.value)\n        else:\n            raise SyntaxError(\"Invalid AST node: {}\".format(ast))\n\n    def interpret_expression(self, expression):\n        \"\"\"\n        Description: Interpret expression\n        :param expression: expression to be interpreted\n        :return: result of interpretation\n        \"\"\"\n        return self.interpret_symbol(expression[0])\n\n    def interpret_symbol(self, symbol):\n        \"\"\"\n        Description: Interpret symbol\n        :param symbol: symbol to be interpreted\n        :return: result of interpretation\n        \"\"\"\n        if symbol.name == '+':\n            return self.interpret_expression(symbol.value) + self.interpret_expression(symbol.value[1:])\n        elif symbol.name == '-':\n            return self.interpret_expression(symbol.value) - self.interpret_expression(symbol.value[1:])\n        elif symbol.name == '*':\n            return self.interpret_expression(symbol.value) * self.interpret_expression(symbol.value[1:])\n        elif symbol.name == '/':\n            return self.interpret_expression(symbol.value) / self.interpret_expression(symbol.value[1:])\n        else:\n            raise SyntaxError(\"Invalid symbol: {}\".format(symbol))\n\n    def interpret_identifier(self, identifier):\n        \"\"\"\n        Description: Interpret identifier\n        :param identifier: identifier to be interpreted\n        :return: result of interpretation\n        \"\"\"\n        return self.ast[identifier.name]\n\n    def __str__(self):\n        return \"Interpreter\"\n\n\ndef lexer_test():\n    \"\"\"\n    Test lexer\n    \"\"\"\n    lexer = Lexer()\n    code = \"123 # comment \\n variables = 5 , 10 # comment \\nprint(5)\"\n    tokens = lexer.parse(code)\n    print(tokens)\n\n\ndef parser_test():\n    \"\"\"\n    Test parser\n    \"\"\"\n    lexer = Lexer()\n    code = \"123 # comment \\n variables = 5 , 10 # comment \\nprint(5)\"\n    tokens = lexer.parse(code)\n    ast = Parser(lexer).parse(tokens)\n    print(ast)\n\n\ndef interpreter_test():\n    \"\"\"\n    Test interpreter\n    \"\"\"\n    ast = AST('expression', ['symbol', '+', 'number', '123', 'number', '5'])\n    interpreter = Interpreter(ast)\n    result = interpreter.interpret()\n    print(result)\n\n\nif __name__ == '__main__':\n    lexer_test()\n    parser_test()\n    interpreter_test()",
